@model IEnumerable<BezvizSystem.Web.Models.Operator.ViewOperatorModel>

@{
    ViewBag.Title = "Туроператоры";
}

<h2>Туроператоры</h2>

<p>@Html.ActionLink("Добавить", "Create")</p>

<table class="table">
    <tr>
    
        <th>
            @Html.DisplayNameFor(model => model.ProfileUserTranscript)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProfileUserUNP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProfileUserOKPO)
        </th>      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.ProfileUserTranscript == null ? "Туроператор" : item.ProfileUserTranscript, "Edit", new { id = item.Id })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProfileUserUNP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProfileUserOKPO)
        </td>     
    </tr>
}
</table>
